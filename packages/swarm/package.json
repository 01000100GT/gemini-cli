{
  "name": "@gemini-cli/swarm",
  "version": "0.1.0",
  "description": "Swarm mode implementation for Gemini CLI",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "build": "node ../../scripts/build_package.js",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prerelease:version": "node ../../scripts/bind_package_version.js",
    "prerelease:deps": "node ../../scripts/bind_package_dependencies.js",
    "prepack": "npm run build"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@gemini-cli/core": "^0.1.0",
    "@google/genai": "^1.4.0",
    "@types/eventemitter3": "^1.2.0",
    "@types/uuid": "^9.0.8",
    "better-sqlite3": "^11.10.0",
    "eventemitter3": "^5.0.1",
    "uuid": "^9.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=18"
  }
}
